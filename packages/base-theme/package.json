{
	"name": "theme-boxuk",
	"version": "1.0.0",
	"description": "WordPress theme for your site.",
	"scripts": {
		"build": "wp-scripts build",
		"format": "wp-scripts format",
		"lint": "npm run lint:css && npm run lint:js",
		"lint:fix": "npm run lint:css:fix && npm run lint:js:fix",
		"lint:css": "wp-scripts lint-style",
		"lint:css:fix": "wp-scripts lint-style --fix",
		"lint:js": "wp-scripts lint-js",
		"lint:js:fix": "wp-scripts lint-js --fix",
		"type-coverage": "type-coverage --strict --detail --at-least 98.8",
		"packages-update": "wp-scripts packages-update",
		"start": "wp-scripts start",
		"test": "npm run test:unit",
		"test:unit": "wp-scripts test-unit-js",
		"test:unit:help": "wp-scripts test-unit-js --help",
		"test:unit:watch": "wp-scripts test-unit-js --watch",
		"test:unit:debug": "wp-scripts --inspect-brk test-unit-js --runInBand --no-cache",
		"test:unit:snapshots": "wp-scripts test-unit-js --updateSnapshot",
		"test:playwright": "wp-scripts test-playwright"
	},
	"author": "Box UK",
	"license": "proprietary",
	"devDependencies": {
		"@jest/globals": "^29.7.0",
		"@playwright/test": "^1.53.1",
		"@wordpress/block-editor": "^13.0.7",
		"@wordpress/blocks": "^13.0.3",
		"@wordpress/dom-ready": "^4.0.1",
		"@wordpress/editor": "^14.0.8",
		"@wordpress/icons": "^10.0.2",
		"@wordpress/scripts": "^28.0.2",
		"jest-junit": "^16.0.0",
		"react": "^18.2.0",
		"type-coverage": "^2.29.7"
	}
}
