		"custom": {
			"breadcrumbs": {
				"font-size": "var(--wp--preset--font-size--small)",
				"gap": "var(--wp--custom--spacing--xxsmall)",
				"separator": "'\\203A'",
				"separator-size": "16px"
			},
			"button:": {
				"border": "2px",
				"radius": "2px",
				"transition": "0.3s ease-in-out"
			},
			"chip": {
				"bg-color": "var(--wp--preset--color--grey-700)",
				"bg-color-hover": "var(--wp--preset--color--grey-900)",
				"font-size": "var(--wp--preset--font-size--small)",
				"spacing": "4px var(--wp--custom--spacing--xsmall)",
				"spacing-large": "var(--wp--custom--spacing--xxsmall) var(--wp--custom--spacing--medium)",
				"text-color": "var(--wp--preset--color--white)"
			},
			"color": {
				"caution": "#F1C21B",
				"error": "#CE112D",
				"focus": "#215AEB",
				"info": "#215AEB",
				"link-enabled": "#215AEB",
				"link-hover": "#006a8d",
				"success": "#198038",
				"warning": "#FF832B"
			},
			"spacing": {
				"large": "2rem",
				"medium": "1.5rem",
				"small": "1rem",
				"xlarge": "clamp(2rem, 1.7727rem + 0.9697vw, 2.5rem)",
				"xsmall": ".75rem",
				"xxlarge": "clamp(2rem, 0.8636rem + 4.8485vw, 4.5rem)",
				"xxsmall": ".5rem",
				"xxxlarge": "clamp(4rem, 2.4091rem + 6.7879vw, 7.5rem)",
				"xxxsmall": ".25rem",
				"xxxxlarge": "clamp(4.5rem, 2.0000rem + 10.6667vw, 10rem)"
			}
		},
		"layout": {
			"contentSize": "760px",
			"wideSize": "1200px"
		},
		"shadow": {
			"defaultPresets": false,
			"presets": [
