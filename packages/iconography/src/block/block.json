{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "boxuk/icon",
	"title": "Icon",
	"category": "media",
	"description": "Display an icon as a block.",
	"icon": "star-filled",
	"keywords": [ "icon", "emoji", "symbol" ],
	"textdomain": "boxuk",
	"style": "file:./style-index.css",
	"editorScript": "file:./index.ts",
	"supports": {
		"anchor": false,
		"align": [],
		"alignWide": false,
		"ariaLabel": true,
		"background": {
			"backgroundImage": false,
			"backgroundSize": false
		},
		"className": true,
		"color": {
			"background": true,
			"text": true
		},
		"customClassName": true,
		"dimensions": {
			"aspectRatio": false,
			"minHeight": false
		},
		"filter": {
			"duotone": false
		},
		"html": false,
		"inserter": true,
		"interactivity": false,
		"lock": true,
		"multiple": true,
		"position": {
			"sticky": false
		},
		"renaming": true,
		"reusable": true,
		"shadow": true,
		"spacing": {
			"margin": true,
			"padding": true,
			"blockGrap": false
		},
		"typography": {
			"fontSize": true,
			"lineHeight": false,
			"textAlign": true
		}
	},
	"attributes": {
		"iconClass": {
			"type": "string"
		},
		"iconTag": {
			"type": "string"
		},
		"iconContent": {
			"type": "string"
		},
		"style": {
			"type": "object",
			"default": {
				"textAlign": "center"
			}
		}
	},
	"example": {}
}
